{
  "plugins": [
    "effector","formatjs"
  ],
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:effector/recommended",
    "plugin:effector/react",
    "plugin:effector/scope"
  ],
  "rules": {
    "import/no-anonymous-default-export": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "formatjs/no-offset": "error"
  }
}
