{
  "name": "cyber-manager-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:init": "npx prisma db push && npx prisma db seed",
    "prisma:format": "npx prisma format",
    "prisma:generate-client": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "postinstall": "yarn prisma:generate-client",
    "predev": "yarn prisma:generate-client",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "build:path": "pathpida --ignorePath .gitignore",
    "build:i18n": "yarn i18n:extract && yarn i18n:translate && run-p i18n:compile:*",
    "prebuild": "yarn build:path && yarn build:i18n",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract \"src/**/*.{js,jsx,ts,tsx}\" --ignore=\"**/*.d.ts\"  --format simple --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file content/locales/ru.json",
    "i18n:translate": "set GOOGLE_APPLICATION_CREDENTIALS=./content/google-translate-key.json&&ts-node ./scripts/translate.ts",
    "i18n:compile:game": "formatjs compile-folder --ast --format simple content/locales public/locales",
    "i18n:compile:heroes": "formatjs compile-folder --ast --format simple content/locales/heroes public/locales/heroes"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/lab": "latest",
    "@mui/material": "latest",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.14.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "effector": "^22.2.0",
    "effector-react": "^22.0.6",
    "effector-storage": "^5.0.0",
    "formik": "^2.2.9",
    "http-status-codes": "^2.2.0",
    "moment": "^2.29.1",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "next-sitemap": "^2.5.20",
    "pathpida": "^0.18.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intl": "^6.0.3",
    "sass": "^1.49.9",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@formatjs/cli": "^5.0.1",
    "@google-cloud/translate": "^6.3.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/socket.io-client": "^3.0.0",
    "babel-plugin-formatjs": "^10.3.23",
    "cross-env": "^7.0.3",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-effector": "^0.6.0",
    "eslint-plugin-formatjs": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prettier-eslint-cli": "^6.0.1",
    "prisma": "^3.14.0",
    "ts-node": "^10.7.0",
    "ts-paths-esm-loader": "^1.3.0",
    "typescript": "4.6.4"
  }
}
